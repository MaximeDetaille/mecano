{
  "name": "react-webpack-rails-tutorial",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "6.9.0",
    "npm": "4.1.1"
  },
  "scripts": {
    "build:test": "webpack --config webpack.client.config.js",
    "build:production:client": "NODE_ENV=production webpack --config webpack.client.config.js",
    "build:production:server": "NODE_ENV=production webpack --config webpack.server.config.js",
    "build:development:client": "webpack -w --config webpack.client.config.js",
    "build:development:server": "webpack -w --config webpack.server.config.js",
    "test": "mocha --compilers js:babel-core/register --require ./app/test/test_helper.js --recursive ./app/test",
    "test:watch": "npm run test -- --watch"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "axios": "^0.16.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.3.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "es5-shim": "^4.5.9",
    "expose-loader": "^0.7.3",
    "history": "^4.6.1",
    "imports-loader": "^0.7.1",
    "intl": "^1.2.5",
    "lodash": "^4.17.4",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-intl": "^2.1.5",
    "react-on-rails": "7.0.4",
    "react-rater": "^0.4.3",
    "react-redux": "^5.0.5",
    "react-redux-toastr": "^6.2.6",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-form": "^6.7.0",
    "redux-persist": "^4.8.0",
    "redux-thunk": "^2.2.0",
    "webpack": "^2.3.3"
  },
  "devDependencies": {
    "list-dependencies": "^1.0.0"
  }
}
